package warmup;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		public static void main(String[] args) {
			Scanner kb = new Scanner(System.in);
			int n = kb.nextInt();
			kb.close();
			for(int i=0; i<n; i++){
				for(int j=1; j<=i; j++){
					System.out.print("*");
				}
			}
		}
	}

}
