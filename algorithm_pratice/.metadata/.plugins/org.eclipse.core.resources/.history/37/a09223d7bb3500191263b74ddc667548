package warmup;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		int N = 0,n=0,a=0;
		Scanner kb = new Scanner(System.in);
		N = kb.nextInt();
		kb.close();
		if(N<5){
			if(N == 3){
				System.out.println("1");
			}
			else{
				System.out.println("-1");
				}
		}
		else {
			if(N%5 == 0){
				System.out.println(N/5);
			}
			else {
				a = N/5;
				n = N - a*5;
				if(n%3 == 0){
					System.out.println(a+ n/3);
				}
				else if(N % 3 == 0) {
					System.out.println(N/3);
				}
				else{
					System.out.println("-1");
				}
			}
		}
		

	}

}
