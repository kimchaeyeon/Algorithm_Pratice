package warmup;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		int a=0,b=0;
		Scanner kb = new Scanner(System.in);
		int N = kb.nextInt();
		kb.close();
		while( N%5!=0 && N>0){
				N -= 3;
				a++;
		}
		if(N%5 == 0){
			System.out.println(N/5);
		}
		else if(N<0){
			System.out.println("-1");
		}
		else{
			System.out.println(a + N/a);
		}

	}

}
